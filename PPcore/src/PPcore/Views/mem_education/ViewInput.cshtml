@model PPcore.Models.mem_education

<form class="form-horizontal">
<div class="form-group">
    <label asp-for="rec_no" class="col-md-3 control-label"></label>
    <div class="col-md-9">
        <input asp-for="rec_no" class="form-control" />
        <span asp-validation-for="rec_no" class="text-danger" />
    </div>
</div>
<div class="form-group">
    <label asp-for="degree" class="col-md-3 control-label"></label>
    <div class="col-md-9">
        <input asp-for="degree" class="form-control" />
        <span asp-validation-for="degree" class="text-danger" />
    </div>
</div>
    <div class="form-group">
        <label asp-for="colledge_name" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="colledge_name" class="form-control" />
            <span asp-validation-for="colledge_name" class="text-danger" />
        </div>
    </div>
    <div class="form-group">
        <label asp-for="faculty" class="col-md-3 control-label"></label>
        <div class="col-md-9">
            <input asp-for="faculty" class="form-control" />
            <span asp-validation-for="faculty" class="text-danger" />
        </div>
    </div>
<div class="form-group">
    <div class="col-md-offset-3 col-md-9">
        <button id="mem_educationSubmit" class="btn btn-success">บันทึก</button>
    </div>
</div>
</form>




@section Scripts {
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}
